<% if current_user %>
                    <li role="presentation" ><%= link_to current_user.email, user_path(current_user) %></li>
                    <li role="presentation"><%= link_to 'Log out', logout_url %></li>
                <% else %>
                    <li role="presentation"><%= link_to 'Log In', login_url %></li>
                    <li role="presentation"><%= link_to 'Register', signup_url %></li>

                <% end %>
                <li role="presentation"><%= link_to 'Home', root_path %></li>

